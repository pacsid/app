<h2>Login</h2>

<%= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>
  <div><%= f.label :email %>
  <%= f.email_field :email, :autofocus => true %></div>

  <div><%= f.label :senha %>
  <%= f.password_field :password %></div>

  <div><%= f.submit "Entrar" %></div>
<% end %>

