<h1>Listing acessos</h1>

<table>
  <thead>
    <tr>
      <th>Server</th>
      <th>Email</th>
      <th>Data</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @acessos.each do |acesso| %>
      <tr>
        <td><%= acesso.server_id %></td>
        <td><%= acesso.email_id %></td>
        <td><%= acesso.data %></td>
        <td><%= link_to 'Show', acesso %></td>
        <td><%= link_to 'Edit', edit_acesso_path(acesso) %></td>
        <td><%= link_to 'Destroy', acesso, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Acesso', new_acesso_path %>
